ifvalid_sensor.exe: win64/projet_operators_dll.o win64/SmuMainWindows.o 
	gcc "win64\projet_operators_dll.o" "win64\SmuMainWindows.o" -lm -lwsock32 -m64 -o "ifvalid_sensor.exe" 

