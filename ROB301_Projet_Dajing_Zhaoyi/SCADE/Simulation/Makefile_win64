win64/SmuProxy.o: D:/tools/ANSYS\ Inc/v202/SCADE/SCADE/lib/C/SmuProxy.c
	gcc -I"$(SCADE)\libraries\SC65\libmathext" -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "D:\tools\ANSYS Inc\v202\SCADE\SCADE\lib\C\SmuProxy.c" -o "win64\SmuProxy.o"

ifvalid_sensor.dll: win64/ifvalid_sensor.o win64/kcg_types.o win64/kcg_consts.o win64/projet_operators_mapping.o win64/projet_operators_type.o win64/SmuPredefConv.o win64/SmuConfig.o win64/SmuTypes.o win64/SmuLib.o win64/SmuVTable.o win64/projet_operators_interface.o win64/SmuProxy.o SmuExport.def 
	gcc -mwindows -mwin32 -shared -static-libgcc  "win64\ifvalid_sensor.o" "win64\kcg_types.o" "win64\kcg_consts.o" "win64\projet_operators_mapping.o" "win64\projet_operators_type.o" "win64\SmuPredefConv.o" "win64\SmuConfig.o" "win64\SmuTypes.o" "win64\SmuLib.o" "win64\SmuVTable.o" "win64\projet_operators_interface.o" "win64\SmuProxy.o" -Wl,--output-def,"SmuExport.def" -lm -lwsock32 -m64 -o "ifvalid_sensor.dll" 

SmuExport.def: D:/tools/ANSYS\ Inc/v202/SCADE/SCADE/lib/C/SmuExport.def
	copy "D:\tools\ANSYS Inc\v202\SCADE\SCADE\lib\C\SmuExport.def" "SmuExport.def"

