E1_Q2.dll: win64/E1_Q2.o win64/kcg_types.o win64/kcg_consts.o win64/TP3_mapping.o win64/TP3_type.o win64/SmuPredefConv.o win64/SmuConfig.o win64/SmuTypes.o win64/SmuLib.o win64/SmuVTable.o win64/TP3_interface.o win64/SmuProxy.o SmuExport.def 
	gcc -mwindows -mwin32 -shared -static-libgcc  "win64\E1_Q2.o" "win64\kcg_types.o" "win64\kcg_consts.o" "win64\TP3_mapping.o" "win64\TP3_type.o" "win64\SmuPredefConv.o" "win64\SmuConfig.o" "win64\SmuTypes.o" "win64\SmuLib.o" "win64\SmuVTable.o" "win64\TP3_interface.o" "win64\SmuProxy.o" -Wl,--output-def,"SmuExport.def" -lm -lwsock32 -m64 -o "E1_Q2.dll" 

SmuExport.def: D:/Program\ Files/ANSYS\ Inc/v211/SCADE\ Student/SCADE/lib/C/SmuExport.def
	copy "D:\Program Files\ANSYS Inc\v211\SCADE Student\SCADE\lib\C\SmuExport.def" "SmuExport.def"

