SCADE=D:\tools\ANSYS Inc\v202\SCADE\SCADE\.

SHELL=cmd.exe

C_FLAGS = -pedantic

all: ifvalid_sensor.exe ifvalid_sensor.dll 

win64/ifvalid_sensor.o: ifvalid_sensor.c
	gcc -I"$(SCADE)\libraries\SC65\libmathext" -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "ifvalid_sensor.c" -o "win64\ifvalid_sensor.o"

win64/kcg_types.o: kcg_types.c
	gcc -I"$(SCADE)\libraries\SC65\libmathext" -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "kcg_types.c" -o "win64\kcg_types.o"

win64/kcg_consts.o: kcg_consts.c
